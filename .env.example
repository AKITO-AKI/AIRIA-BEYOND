# API Keys (Required)
# Get your API token from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Get your API token from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Set to any value to disable external image generation
# DISABLE_EXTERNAL_GENERATION=true

# Optional: Set to any value to disable LLM and force rule-based fallback
# DISABLE_LLM_ANALYSIS=true

# --- Local-first AI (Optional) ---
# Prefer local providers over paid APIs
# LLM_PROVIDER=ollama
# IMAGE_PROVIDER=comfyui

# Ollama
# OLLAMA_BASE_URL=http://127.0.0.1:11434
# OLLAMA_MODEL=qwen2.5:7b-instruct
# OLLAMA_MODEL_CHAT=qwen2.5:7b-instruct
# OLLAMA_MODEL_BRIEF=qwen2.5:7b-instruct
# OLLAMA_MODEL_MUSIC=qwen2.5:7b-instruct

# ComfyUI
# COMFYUI_BASE_URL=http://127.0.0.1:8188
# COMFYUI_CHECKPOINT=sdxl_base_1.0.safetensors
# COMFYUI_WIDTH=1024
# COMFYUI_HEIGHT=1024
# COMFYUI_STEPS=25
# COMFYUI_CFG=7.5
# COMFYUI_SAMPLER=euler
# COMFYUI_SCHEDULER=normal
# COMFYUI_TIMEOUT_MS=180000
# COMFYUI_POLL_INTERVAL_MS=750

# Debug: write request/response payloads under .debug/ai
# DEBUG_AI=1

VITE_API_BASE_URL=https://airia-beyond.onrender.com

# Monitoring (Optional)
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_ENABLE_ANALYTICS=true

# Admin (Optional)
ADMIN_TOKEN=your_admin_token_here
